{
  "Path": "/api/v1/city",
  "ClientOutput": {
    "TypeScript": {
      "File": "${Project}/web/api/gapi-city.ts"
    }
  },
  "ServerOutput": {
    "Golang": {
      "Package": "city",
      "File": "gapi-city.go"
    }
  },
  "Import": {
    "Geo": {
      "From": "../root",
      "Class": "GeoLocation"
    }
  },
  "Class": {
    "City": {
      "Name": "String:required",
      "Location": "GeoLocation"
    },
    "PersonList": "List<Person>",
    "PersonMap": "Map<Person>"
  },
  "Action": {
    "GetPerson": {
      "HttpMethod": "GET",
      "Args": [
        {
          "Name": "id",
          "Type": "String"
        }
      ],
      "Return": ["Persion", "Error"]
    }
  }
}
