{
  "version": "gapi.v1",
  "path": "/api/v1/city",
  "output": [
    {
      "kind": "client",
      "language": "typescript",
      "package": "city",
      "file": "gapi-city.go"
    },
    {
      "kind": "server",
      "language": "golang",
      "package": "city",
      "file": "gapi-city.go"
    }
  ],
  "class": {
    "GeoLocation": {
      "from": "../root",
      "name": "Geo"
    },
    "City": {
      "name": {
        "type": "String",
        "required": true
      },
      "location": {
        "type": "GeoLocation",
        "required": true
      }
    }
  },
  "action": {
    "GetCity": {
      "method": "GET",
      "args": [
        {
          "name": "id",
          "type": "String",
          "required": true
        }
      ],
      "return": ["City", "Error"]
    }
  }
}
