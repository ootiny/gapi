{
  "version": "gapi.v1",
  "apiPath": "/api/v1/city",
  "outputs": [
    {
      "kind": "client",
      "language": "typescript",
      "package": "city",
      "filePath": "gapi-city.ts"
    },
    {
      "kind": "server",
      "language": "golang",
      "package": "city",
      "filePath": "gapi-city.go"
    }
  ],
  "definitions": {
    "GeoLocation": {
      "from": "../root",
      "name": "Geo"
    },
    "City": {
      "name": {
        "type": "String",
        "required": true
      },
      "location": {
        "type": "GeoLocation",
        "required": true
      }
    }
  },
  "actions": {
    "GetCity": {
      "method": "GET",
      "parameters": [
        {
          "name": "id",
          "type": "String",
          "required": true
        }
      ],
      "returns": ["City", "Error"]
    }
  }
}
