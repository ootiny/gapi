{
  "version": "gapi.v1",
  "apiPath": "/",
  "outputs": [
    {
      "kind": "client",
      "language": "typescript",
      "filePath": "${ProjectDir}/web/gapi/root.ts"
    },
    {
      "kind": "server",
      "language": "golang",
      "package": "gapi",
      "filePath": "${ProjectDir}/server/gapi/root.go"
    }
  ],
  "definitions": {
    "Geo": {
      "attributes": [
        {
          "name": "latitude",
          "type": "Float64",
          "required": true,
          "description": "The latitude of the geo location"
        },
        {
          "name": "longitude",
          "type": "Float64",
          "required": true,
          "description": "The longitude of the geo location"
        }
      ]
    }
  }
}
